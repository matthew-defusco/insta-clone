{
  "name": "insta-clone",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "devserver": "npm run dev --prefix server",
    "devclient": "npm run dev --prefix client",
    "dev": "concurrently \"npm run devserver\" \"npm run devclient\"",
    "clientinstall": "npm install --prefix client",
    "serverinstall": "npm install --prefix server",
    "install": "npm run clientinstall && npm run serverinstall",
    "clientbuild": "npm run build --prefix client",
    "build": "npm run install && npm run clientbuild",
    "start": "node index.js --prefix server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
